@page
@model OrtizAbrahamSprint3.Pages.SignOutModel
@{
    ViewData["Title"] = "Sign Out";
}

@*Title rows*@
<div class="sign-up-title log-out-title">
    <h1>Are you sure you want to log out?</h1>
</div>


<form method="post" class="all-data-container log-out-container">
    @*Buttons rows*@
    <div class="data-buttons-container">
        <input asp-page-handler="SignOut" type="submit" value="Log Out" class="submit-button" />
        <input asp-page-handler="Cancel" type="submit" value="Cancel" class="clear-button" />
    </div>
</form>